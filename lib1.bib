	Issn = {0167-577X},
	Journal = {Mater. Lett.},
	Keywords = {growth, Luminescence, mu-PL spectra, Nanoribbons, nanowires, photoluminescence, Semiconductors, Structure defects, temperature, vapor-phase deposition, ZnSe},
	Language = {English},
	Month = aug,
	Note = {WOS:000338818800033},
